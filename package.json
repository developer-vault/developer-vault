{
  "name": "developer-vault",
  "version": "0.1.0",
  "description": "All-in-one toolbox",
  "main": ".dev/main/app.min.js",
  "homepage": "https://github.com/developer-vault/developer-vault",
  "repository": "https://github.com/developer-vault/developer-vault.git",
  "author": "developer-vault",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "dotenv -e .env.test -e .env jest --",
    "test:coverage": "dotenv -e .env.test -e .env jest -- --collect-coverage",
    "test:debugger": "dotenv -e .env.test -e .env node -- --inspect-brk=9555 ./node_modules/jest/bin/jest.js --runInBand",
    "dev": "run-s _clean:dev _run:dev",
    "build": "run-s _clean:dist _build:dist _copy:dist && cd .dist && yarn install --production",
    "packager": "electron-builder --config .electronbuilderrc.js",
    "clean": "run-p _clean:dev _clean:dist _clean:releases _clean:reports _clean:tmp",
    "lint": "run-p lint:**",
    "lint:js": "eslint . --ext=js --ext=jsx",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint-fix": "run-p lint-fix:**",
    "lint-fix:js": "run-s \"lint:js --fix\"",
    "lint-fix:scss": "run-s \"lint:scss --fix\"",
    "i18n:update": "run-s _i18n:extractMessages _i18n:generatePot _i18n:updatePo _i18n:cleanPo _clean:i18n",
    "storybook": "start-storybook -p 9001 -c .storybook --quiet",
    "storybook:build": "build-storybook -c .storybook -o .storybook-build/",
    "_i18n:extractMessages": "node scripts/intl.js -a extract:messages -p 'src/renderer/**/*.messages.{js,jsx}'",
    "_i18n:generatePot": "react-intl-po json2pot .tmp/i18n/messages.json -o .tmp/i18n/messages.pot -c id",
    "_i18n:updatePo": "node scripts/intl.js -a update:pofiles -p 'src/renderer/locales/**/*.po' -t .tmp/i18n/messages.pot",
    "_i18n:cleanPo": "node scripts/intl.js -a clean:pofiles -p 'src/renderer/locales/**/*.po'",
    "_run:dev": "run-p _run:main:dev _serve:renderer:dev",
    "_run:main:dev": "run-p _watch:main:dev _run:main:dev:wait",
    "_build:main:dev": "dotenv -e .env.local -e .env webpack -- --config webpack.config.main.js --mode development",
    "_watch:main:dev": "dotenv -e .env.local -e .env webpack -- --config webpack.config.main.js --mode development --watch",
    "_run:main:dev:wait": "just-wait -p ./.dev/main/** && nodemon --inspect --watch ./.dev/main --exec \"npx dotenv -e .env.local -e .env electron --\" .",
    "_serve:renderer:dev": "dotenv -e .env.local -e .env webpack-dev-server -- --config webpack.config.renderer.js --mode development",
    "_build:renderer:dev": "dotenv -e .env.local -e .env webpack -- --config webpack.config.renderer.js --mode development",
    "_build:dist": "run-p _build:main:dist _build:renderer:dist",
    "_build:main:dist": "dotenv -e .env.local -e .env webpack -- --config webpack.config.main.js --mode production",
    "_build:renderer:dist": "dotenv -e .env.local -e .env webpack -- --config webpack.config.renderer.js --mode production",
    "_copy:dist": "cpy package.json yarn.lock ./.dist",
    "_clean:dev": "rimraf --no-glob .dev",
    "_clean:dist": "rimraf --no-glob .dist",
    "_clean:releases": "rimraf --no-glob .releases",
    "_clean:reports": "rimraf --no-glob .reports",
    "_clean:tmp": "rimraf --no-glob .tmp",
    "_clean:i18n": "rimraf --no-glob .tmp/i18n"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@sentry/browser": "^5.6.3",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "cpy-cli": "^2.0.0",
    "css-loader": "^3.2.0",
    "dotenv-cli": "^2.0.1",
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.9.8",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-react-intl": "^0.7.1",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "just-wait": "^1.0.11",
    "lint-staged": "^9.4.1",
    "memoize-one": "^5.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mock-fs": "^4.10.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.7.2",
    "rc-form": "^2.4.9",
    "react": "^16.10.1",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.10.1",
    "react-intl": "^3.3.2",
    "react-intl-po": "^2.2.2",
    "react-intl-po-loader": "^1.0.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "reapop": "^2.1.0",
    "reapop-theme-wybo": "^1.0.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.4.0",
    "sass-extract-loader": "^1.1.0",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "storybook-addon-intl": "^2.4.1",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.11.1",
    "stylelint-webpack-plugin": "^1.0.1",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^2.1.2",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^14.0.0",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "electron-devtools-installer": "^2.2.4",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "regenerator-runtime": "^0.13.3"
  }
}
